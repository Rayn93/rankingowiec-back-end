{% extends "RankingowiecBundle::base.html.twig" %}

{% block pageTitle %} Oceniaj wszystko i wszystkich! {% endblock %}

{% block pageDescription %} Portal z rankingami, oceniaj sławnych ludzi, filmy, youtuberów czy najlepsze piwo! Dowiedz się co inni myślą. {% endblock %}

{% block stylesheets %}
    <link href='http://fonts.googleapis.com/css?family=Pragati+Narrow:700,400&subset=latin,latin-ext' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="{{ asset('assets/css/html5reset-1.6.1.css') }}" />
    <link rel="stylesheet" href="{{ asset('assets/css/bootstrap.min.css') }}" />
    <link rel="stylesheet" href="{{ asset('assets/css/font-awesome.min.css') }}" />
    <link rel="stylesheet" href="{{ asset('assets/css/select2.min.css') }}" />
    <link rel="stylesheet" href="{{ asset('assets/css/style_main.css') }}" />
    <link rel="stylesheet" href="{{ asset('assets/css/media-queries.css') }}" />
{% endblock %}

{% block bodyId %} rank-list {% endblock %}

{% block topImage %}
    <!--Carousel top-->
    <div class="container-fluid">
        <div class="row">
            <div id="myCarousel" class="carousel slide" data-ride="carousel">
                <!-- Carousel indicators -->
                <ol class="carousel-indicators">
                    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                    <li data-target="#myCarousel" data-slide-to="1"></li>
                    <li data-target="#myCarousel" data-slide-to="2"></li>
                    <li data-target="#myCarousel" data-slide-to="3"></li>
                    <li data-target="#myCarousel" data-slide-to="4"></li>
                </ol>
                <!-- Wrapper for carousel items -->
                <div class="carousel-inner ">
                    <div class="item active">
                        <img src="img/slajdy/slide0.jpg" alt="slajd 1"/>

                        <div class="carousel-caption">
                            <h3>Największy patriota w hostorii Polski</h3>
                            <a class="button" href="#"><span>ZOBACZ RANKING</span></a>
                        </div>
                    </div>
                    <div class="item">
                        <a href="#"><img class="img-responsive" src="img/slajdy/slide1.jpg" alt="slajd 2"/></a>

                        <div class="carousel-caption">
                            <h3>Polak, który najbardziej przyczynił się do rozwoju polski</h3>
                            <a class="button" href="#"><span>ZOBACZ RANKING</span></a>
                        </div>
                    </div>
                    <div class="item">
                        <a href="#"><img src="img/slajdy/slide2.jpg" alt="slajd 3"/></a>

                        <div class="carousel-caption">
                            <h3>Najpiękniejsza polska aktorka</h3>
                            <a class="button" href="#"><span>ZOBACZ RANKING</span></a>
                        </div>
                    </div>
                    <div class="item">
                        <a href="#"><img src="img/slajdy/slide3.jpg" alt="slajd 3"/></a>

                        <div class="carousel-caption">
                            <h3>Najlepszy polski olimpijczyk w Rio [2016]</h3>
                            <a class="button" href="#"><span>ZOBACZ RANKING</span></a>
                        </div>
                    </div>
                    <div class="item">
                        <a href="#"><img src="img/slajdy/slide4.jpg" alt="slajd 4"/></a>

                        <div class="carousel-caption">
                            <h3>Kto w 2016 roku rządzi polską?</h3>
                            <a class="button" href="#"><span>ZOBACZ RANKING</span></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--end carousel-->

{% endblock %}

{% block underImage %}

    <!--nav bar under carouse-->
    <div class="page-title">
        <h1><i class="fa fa-film" aria-hidden="true"></i> NAJLEPSZE FILMY </h1>
    </div>

{% endblock %}

